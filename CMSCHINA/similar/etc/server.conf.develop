[server]
#服务名(全称)：命名空间$服务名简称, client端请求server的标识 
service=xinhuanet$Nlp$Similar
#server服务端口
port=9112
#direct_address=172.18.1.146:9002
#zookeeper的连接字符串
zk_connect_str=127.0.0.1:2181
#zk_connect_str=172.24.4.18:2181,172.24.4.19:2181,172.24.4.20:2181
#server授权用户字符串 避免其他服务也注册此服务
auth_user=similar
#server授权用户字符串 避免其他服务也注册此服务名
auth_password=similar
#是否监控server的状态和请求数等信息， 默认值是False
monitor=True
#server服务的名称
name=xinhuanetNlpSimilar
#server 服务负责人 
owner=xia.hong@baifendian.com
#server 开启的进程数 默认值10个
process_num=3
#server 每个进程中的协程数目，默认值100个
coroutines_num=6
